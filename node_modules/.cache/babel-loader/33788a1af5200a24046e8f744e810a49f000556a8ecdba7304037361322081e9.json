{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.filter.js\";\nexport default {\n  name: \"RentalCatalog\",\n  data() {\n    return {\n      searchQuery: \"\",\n      cats: [{\n        id: 1,\n        name: \"Барсик\",\n        price: 100,\n        photo: \"photo1.jpg\",\n        age: 2,\n        color: \"Белый\",\n        character: \"Ласковый\"\n      }, {\n        id: 2,\n        name: \"Мурзик\",\n        price: 200,\n        photo: \"photo2.jpg\",\n        age: 4,\n        color: \"Черный\",\n        character: \"Агрессивный\"\n      }, {\n        id: 3,\n        name: \"Пушок\",\n        price: 150,\n        photo: \"photo3.jpg\",\n        age: 3,\n        color: \"Рыжий\",\n        character: \"Послушный\"\n      }, {\n        id: 4,\n        name: \"Снежок\",\n        price: 120,\n        photo: \"photo4.jpg\",\n        age: 1,\n        color: \"Серый\",\n        character: \"Игривый\"\n      }, {\n        id: 5,\n        name: \"Василиса\",\n        price: 180,\n        photo: \"photo5.jpg\",\n        age: 5,\n        color: \"Белый\",\n        character: \"Независимый\"\n      }]\n    };\n  },\n  computed: {\n    filteredCats() {\n      // Фильтрация по поисковому запросу\n      return this.cats.filter(cat => {\n        const searchText = this.searchQuery.toLowerCase();\n        return cat.name.toLowerCase().includes(searchText) || cat.character.toLowerCase().includes(searchText) || cat.color.toLowerCase().includes(searchText);\n      });\n    }\n  },\n  methods: {\n    getCatPhoto(photo) {\n      return require(`@/assets/photos/${photo}`);\n    },\n    addToCart(cat) {\n      const cart = JSON.parse(localStorage.getItem(\"cart\")) || [];\n      cart.push({\n        ...cat\n      });\n      localStorage.setItem(\"cart\", JSON.stringify(cart));\n      this.$router.push(\"/checkout\");\n    }\n  }\n};","map":{"version":3,"names":["name","data","searchQuery","cats","id","price","photo","age","color","character","computed","filteredCats","filter","cat","searchText","toLowerCase","includes","methods","getCatPhoto","require","addToCart","cart","JSON","parse","localStorage","getItem","push","setItem","stringify","$router"],"sources":["/Users/sergey/Desktop/Политех/Web/PWA/src/pages/RentalCatalog.vue"],"sourcesContent":["<template>\n  <div class=\"catalog\">\n    <input\n      type=\"text\"\n      v-model=\"searchQuery\"\n      placeholder=\"Поиск по имени или характеристикам...\"\n      class=\"search-input\"\n    />\n    <div v-for=\"cat in filteredCats\" :key=\"cat.id\" class=\"cat-item\">\n      <img :src=\"getCatPhoto(cat.photo)\" alt=\"Cat image\" class=\"cat-photo\" />\n      <div class=\"cat-info\">\n        <h3>{{ cat.name }}</h3>\n        <p>Цена: {{ cat.price }} ₽</p>\n        <p>Возраст: {{ cat.age }} года</p>\n        <p>Окраска: {{ cat.color }}</p>\n        <p>Характер: {{ cat.character }}</p>\n        <button @click=\"addToCart(cat)\">Добавить в корзину</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"RentalCatalog\",\n  data() {\n    return {\n      searchQuery: \"\",\n      cats: [\n        { \n          id: 1, name: \"Барсик\", price: 100, photo: \"photo1.jpg\", \n          age: 2, color: \"Белый\", character: \"Ласковый\" \n        },\n        { \n          id: 2, name: \"Мурзик\", price: 200, photo: \"photo2.jpg\", \n          age: 4, color: \"Черный\", character: \"Агрессивный\" \n        },\n        { \n          id: 3, name: \"Пушок\", price: 150, photo: \"photo3.jpg\", \n          age: 3, color: \"Рыжий\", character: \"Послушный\" \n        },\n        { \n          id: 4, name: \"Снежок\", price: 120, photo: \"photo4.jpg\", \n          age: 1, color: \"Серый\", character: \"Игривый\" \n        },\n        { \n          id: 5, name: \"Василиса\", price: 180, photo: \"photo5.jpg\", \n          age: 5, color: \"Белый\", character: \"Независимый\" \n        },\n      ],\n    };\n  },\n  computed: {\n    filteredCats() {\n      // Фильтрация по поисковому запросу\n      return this.cats.filter((cat) => {\n        const searchText = this.searchQuery.toLowerCase();\n        return (\n          cat.name.toLowerCase().includes(searchText) ||\n          cat.character.toLowerCase().includes(searchText) ||\n          cat.color.toLowerCase().includes(searchText)\n        );\n      });\n    },\n  },\n  methods: {\n    getCatPhoto(photo) {\n      return require(`@/assets/photos/${photo}`);\n    },\n    addToCart(cat) {\n      const cart = JSON.parse(localStorage.getItem(\"cart\")) || [];\n      cart.push({ ...cat });\n      localStorage.setItem(\"cart\", JSON.stringify(cart));\n      this.$router.push(\"/checkout\");\n    },\n  },\n};\n</script>\n\n<style scoped>\n.catalog {\n  padding: 20px;\n  display: grid;\n  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\n  gap: 20px;\n}\n\n.search-input {\n  width: 100%;\n  padding: 10px;\n  margin-bottom: 20px;\n  font-size: 1em;\n  border: 1px solid #ccc;\n  border-radius: 5px;\n}\n\n.cat-item {\n  background-color: #fff;\n  border: 1px solid #ddd;\n  border-radius: 10px;\n  overflow: hidden;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\n}\n\n.cat-photo {\n  width: 100%;\n  height: auto;\n}\n\n.cat-info {\n  padding: 15px;\n}\n\n.cat-info h3 {\n  font-size: 1.5em;\n  color: #333;\n}\n\n.cat-info p {\n  margin: 5px 0;\n  color: #555;\n}\n\nbutton {\n  background-color: #42b983;\n  color: white;\n  border: none;\n  padding: 10px 15px;\n  font-size: 1em;\n  border-radius: 5px;\n  cursor: pointer;\n}\n\nbutton:hover {\n  background-color: #379d72;\n}\n</style>\n"],"mappings":";;;AAuBA,eAAe;EACbA,IAAI,EAAE,eAAe;EACrBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,EAAE;MACfC,IAAI,EAAE,CACJ;QACEC,EAAE,EAAE,CAAC;QAAEJ,IAAI,EAAE,QAAQ;QAAEK,KAAK,EAAE,GAAG;QAAEC,KAAK,EAAE,YAAY;QACtDC,GAAG,EAAE,CAAC;QAAEC,KAAK,EAAE,OAAO;QAAEC,SAAS,EAAE;MACrC,CAAC,EACD;QACEL,EAAE,EAAE,CAAC